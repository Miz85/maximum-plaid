{{yield (hash
  line=(component "plaid-line" xScale=xScale yScale=yScale x=plotArea.left y=plotArea.top)
  area=(component "plaid-area" xScale=xScale yScale=yScale x=plotArea.left y=plotArea.top)
  scatter=(component "plaid-scatter" xScale=xScale yScale=yScale x=plotArea.left y=plotArea.top)
  symbol=(component "plaid-symbol" x=plotArea.left y=plotArea.top)
  bottom-axis=(component "plaid-axis" orientation="bottom" scale=xScale y=plotArea.bottom x=plotArea.left)
  donut=(component "plaid-donut" outerRadius=plotArea.height innerRadius=(sub plotArea.height 32))
)}}
